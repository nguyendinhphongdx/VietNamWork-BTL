
@{
    ViewBag.Title = "edit_company";
    Layout = "~/Views/Layout/_adminLayout.cshtml";
}
<script src="~/Content/assets/back-end/js/edit-company.js"></script>
<div class="title">
    Cập Nhật Công Ty
</div>
<div class="table-user">
    <div class="box-form">
        <form action="" onsubmit="return false">
            <div class="row">
                <img src="~/Content/assets/back-end/images/2.png" alt="" style="max-width:100px" id="imgsrc">
                <label for="">Picture</label>
                <input type="file" id="image">
            </div>
            <div class="row">
                <label for="">Name</label>
                <input type="text" placeholder="Name" id="name">
            </div>
            <div class="row">
                <label for="">Address</label>
                <input type="text" placeholder="Address" id="address">
            </div>
            <div class="row" id="disable">
                <label for="">Status</label>
                <input type="checkbox" name="disable" id="idisable" value="not">
                <label id="lbdi" for="idisable">Still</label>
            </div>
            <div class="row">
                <input type="submit" name="" id="" value="Update Company" onclick="UpdateCompany()">
            </div>
        </form>
    </div>

</div>
